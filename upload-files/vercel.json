{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/vite.svg",
      "dest": "/vite.svg"
    },
    {
      "src": "/kanban",
      "dest": "/index.html"
    },
    {
      "src": "/calendar",
      "dest": "/index.html"
    },
    {
      "src": "/timeline",
      "dest": "/index.html"
    },
    {
      "src": "/list",
      "dest": "/index.html"
    },
    {
      "src": "/settings",
      "dest": "/index.html"
    },
    {
      "src": "/profile",
      "dest": "/index.html"
    },
    {
      "src": "/login",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 